{
  "name": "svelfhir",
  "description": "A fhir client made with Svelte",
  "version": "1.0.0",
  "scripts": {
    "build": "rollup -c .config/rollup.config.js",
    "dev": "rollup -c .config/rollup.config.js -w",
    "start": "sirv public -s --port 3000",
    "validate": "svelte-check",
    "storybook": "start-storybook -p 6006 -c .config/storybook",
    "build-storybook": "build-storybook -c .config/storybook"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.3.6",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "babel-loader": "^8.2.2",
    "i18next-json-sync": "^2.3.1",
    "luxon": "^2.0.1",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "simplemde": "^1.11.2",
    "svelte": "^3.41.0",
    "svelte-check": "^1.0.0",
    "svelte-i18n": "^3.3.9",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.0.0",
    "svelte-windicss-preprocess": "^2.1.9",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "windicss-webpack-plugin": "^1.4.1"
  },
  "dependencies": {
    "node-sass": "^6.0.1",
    "sirv-cli": "^1.0.0"
  }
