{
  "name": "svendistoje",
  "description": "A base setup for Svelte with WindiCSS, Storybook and Jest in Typescript",
  "version": "1.0.1",
  "scripts": {
    "build": "rollup -c .config/rollup.config.js",
    "clean": "rimraf public/build",
    "dev": "rollup -c .config/rollup.config.js -w",
    "start": "sirv public -s --port 3000",
    "validate": "svelte-check",
    "storybook": "start-storybook -p 6006 -c .config/storybook",
    "build-storybook": "build-storybook -c .config/storybook",
    "test": "jest -c .config/jest.config.js",
    "test:watch": "npm test -- --watch"
  },
  "devDependencies": {
    "@babel/core": "^7.14.8",
    "@babel/preset-env": "^7.14.8",
    "@fortawesome/fontawesome-free": "^5.15.3",
    "@rollup/plugin-commonjs": "^17.0.0",
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-replace": "^3.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@storybook/addon-actions": "^6.3.6",
    "@storybook/addon-essentials": "^6.3.6",
    "@storybook/addon-links": "^6.3.6",
    "@storybook/addon-svelte-csf": "^1.1.0",
    "@storybook/svelte": "^6.3.6",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/svelte": "^3.0.3",
    "@tsconfig/svelte": "^1.0.0",
    "@types/jest": "^26.0.24",
    "babel-jest": "^27.0.6",
    "babel-loader": "^8.2.2",
    "i18next-json-sync": "^2.3.1",
    "jest": "^27.0.6",
    "jest-svelte-events": "^1.0.5",
    "rollup": "^2.3.4",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-livereload": "^2.0.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.41.0",
    "svelte-check": "^1.0.0",
    "svelte-i18n": "^3.3.9",
    "svelte-jester": "^1.7.0",
    "svelte-loader": "^3.1.2",
    "svelte-preprocess": "^4.7.4",
    "svelte-windicss-preprocess": "^2.1.9",
    "ts-jest": "^27.0.4",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0",
    "windicss-webpack-plugin": "^1.6.0",
    "rimraf": "^3.0.2",
    "sirv-cli": "^1.0.0"
  },
  "dependencies": {}
}
